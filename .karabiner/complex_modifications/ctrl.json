        }
        ],
        "type": "basic"
      },
      {
        "from": {
          "key_code": "p",
          "modifiers": {
            "mandatory": [
              "left_control"
            ],
            "optional": [
              "any"
            ]
          }
        },
        "to": [
        {
          "key_code": "up_arrow"
        }
        ],
        "type": "basic"
      },
      {
        "from": {
          "key_code": "f",
          "modifiers": {
            "mandatory": [
              "left_control"
            ],
            "optional": [
              "any"
            ]
